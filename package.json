{
  "name": "horizontal-timeline",
  "version": "1.0.1",
  "description": "An easy to customize, horizontal timeline powered by CSS and jQuery.",
  "main": "js/main.js",
  "scripts": {
    "css": "npm-run-all --parallel css-compile* --sequential css-prefix* css-minify*",
    "css-compile": "npm-sass scss/style.scss > scss/style-tmp.scss && node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/style-tmp.scss dist/css/style.css && rm scss/style-tmp.scss",
    "css-prefix": "postcss --config build/postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/style.min.css dist/css/style.css",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humantech/horizontal-timeline.git"
  },
  "keywords": [
    "timeline",
    "horizontal",
    "timeline",
    "jquery"
  ],
  "author": "Ariane Rocha <ariane.rocha@collabo.com.br>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/humantech/horizontal-timeline/issues"
  },
  "homepage": "https://github.com/humantech/horizontal-timeline#readme",
  "dependencies": {
    "bourbon": "^5.1.0",
    "jquery": "^3.3.1"
  },
  "files": [
    "dist/",
    "js/",
    "scss/"
  ],
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "clean-css-cli": "^4.1.11",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "npm-sass": "^2.3.0",
    "postcss-cli": "^6.0.0"
  }
}
